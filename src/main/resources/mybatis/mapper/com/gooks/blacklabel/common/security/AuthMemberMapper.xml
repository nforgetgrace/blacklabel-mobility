<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gooks.blacklabel.common.security.domain.AuthMemberMapper">
    <select id="findByMemberId"  
    resultType="com.gooks.blacklabel.common.security.domain.AuthMember">
		SELECT A.MEMBER_ID, A.MEMBER_PASSWORD, B.ROLES
		FROM MEMBER AS A, MEMBER_ROLES AS B
		WHERE A.MEMBER_ID = #{request};
    </select>
</mapper>